/* pages/user/partnerShare/partnerShare.wxss */
.partnerShare {
  width: 100%;
}
.partnerShare .table {
  font-family: SourceHanSansCN-Normal;
	font-size: 28rpx;
}
.partnerShare .table .th, .partnerShare .table .tr {
  display: flex;
  border-bottom: 1rpx solid #e9e9e9;
}
.partnerShare .table .td {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
}
.partnerShare .table .th {
  height: 80rpx;
  align-items: center;
  background: #f8f8f8
}
.partnerShare .table .tr {
  min-height: 80rpx;
  padding: 10rpx 0;
  color: #636363;
}
.partnerShare .table .tr .td1 {
  color: #18425a;
}