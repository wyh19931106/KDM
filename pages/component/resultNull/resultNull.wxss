/* pages/component/resultNull/resultNull.wxss */
.result_win {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 99;
  background: none;
}
.result_win .result_box {
  position: absolute;
  bottom: 150rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 640rpx;
	height: 554rpx; 
  overflow: hidden;
  background: #ffffff;
	box-shadow: 0px 0px 9rpx 2rpx rgba(0, 0, 0, 0.3);
}
.result_win .result_box .result_empty {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 100%;
}
.result_win .result_box .result_empty .title {
  font-family: SourceHanSansCN-Normal;
	font-size: 46rpx;
  color: #18425a;
}
.result_win .result_box .result_empty .msg {
  margin-top: 50rpx;
  font-family: SourceHanSansCN-Normal;
	font-size: 38rpx;
  color: #9a9a9a;
}
.result_win .result_box .result_empty .confirm {
  width: 168rpx;
	height: 78rpx;
	background: #18425a;
	border-radius: 10rpx;
  margin-top: 40rpx;
  color: #ffffff;
  display: flex;
  justify-content: center;
  align-items: center;
  font-family: SourceHanSansCN-Normal;
	font-size: 38rpx;
}