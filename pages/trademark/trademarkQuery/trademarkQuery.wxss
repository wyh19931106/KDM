/* pages/trademark/trademarkQuery/trademarkQuery.wxss */
.trademarkQuery .query {
  margin: 10rpx;
  height: 68rpx;
  border-radius: 10rpx;
  display: flex;
  align-items: center;
  border: 1rpx solid #e9e9e9;
  overflow: hidden;
  font-size: 30rpx;
}
.trademarkQuery .query .country {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 130rpx;
  height: 100%;
  color: #9a9a9a;
  border-right: 1rpx solid #e9e9e9;
  padding-right: 50rpx;
  background: url('data:image/png;base64,') 93rpx center no-repeat;
  background-size: 22rpx auto;
}
.trademarkQuery .query .country text {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin: 0 0 0 8rpx;
}
.trademarkQuery .query .cate {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 180rpx;
  height: 100%;
  color: #9a9a9a;
  border-right: 1rpx solid #e9e9e9;
  padding-right: 50rpx;
  background: url('data:image/png;base64,') 143rpx center no-repeat;
  background-size: 22rpx auto;
}
.trademarkQuery .query .cate text {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin: 0 0 0 8rpx;
}
.trademarkQuery .query .text {
  flex: 1;
  margin: 0 15rpx;
}
.trademarkQuery .query .btn {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 140rpx;
  height: 100%;
  color: #ffffff;
  background: #18425a;
}
.trademarkQuery .query .btn icon {
  margin-right: 8rpx;
}
.trademarkQuery .result_y {
  width: 100%;
}
.trademarkQuery .result_y .hint {
  padding: 10rpx 30rpx;
  font-family: SourceHanSansCN-Normal;
	font-size: 24rpx;
  color: #9a9a9a;
}
.trademarkQuery .result_y .table {
  margin: 10rpx;
  border-top: 1rpx solid #e9e9e9;
  border-left: 1rpx solid #e9e9e9;
}
.trademarkQuery .result_y .table .tr {
  display: flex;
  height: 110rpx;
}
.trademarkQuery .result_y .table .tr .td {
  display: flex;
  align-items: center;
  height: 100%;
  font-family: SourceHanSansCN-Normal;
	font-size: 24rpx;
  color: #9a9a9a;
  border-right: 1rpx solid #e9e9e9;
  border-bottom: 1rpx solid #e9e9e9;
}
.trademarkQuery .result_y .table .tr .td1 {
  width: 220rpx;
  padding: 0 20rpx;
}
.trademarkQuery .result_y .table .tr .td2 {
  flex: 1;
  display: flex;
  align-items: center;
  overflow: hidden;
  padding: 0 20rpx;
}
.trademarkQuery .result_y .table .tr .td2 text {
  flex: 1;
}
.trademarkQuery .result_y .table .tr .td2 .r {
  width: 15rpx;
  height: auto;
  margin-left: 30rpx;
}
.trademarkQuery .result_y .list {
  background: #f8f8f8;
  padding: 15rpx;
  margin-top: 20rpx;
}
.trademarkQuery .result_y .list .item {
  display: flex;
  height: 220rpx;
  padding: 15rpx;
  border-radius: 10rpx;
  background: #ffffff;
  overflow: hidden;
  margin-bottom: 20rpx;
}
.trademarkQuery .result_y .list .item .img {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 190rpx;
  height: 190rpx;
  overflow: hidden;
}
.trademarkQuery .result_y .list .item .img image {
  width: 100%;
  height: auto;
}
.trademarkQuery .result_y .list .item .inf {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  margin: 0 30rpx;
  overflow: hidden;
}
.trademarkQuery .result_y .list .item .inf view {
  font-family: SourceHanSansCN-Normal;
	font-size: 24rpx;
  color: #9a9a9a;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.trademarkQuery .result_y .list .item .inf .tit {
  font-family: SourceHanSansCN-Normal;
	font-size: 30rpx;
  color: #18425a;
}
.trademarkQuery .result_y .list .item .fav {
  width: 120rpx;
  font-family: SourceHanSansCN-Normal;
	font-size: 24rpx;
  color: #18425a;
  text-align: right;
  padding-top: 5rpx;
}
