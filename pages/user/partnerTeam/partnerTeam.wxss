/* pages/user/partnerTeam/partnerTeam.wxss */
.partnerTeam {
  width: 100%;
  height: auto;
  padding-bottom: 100rpx;
}
.partnerTeam .top_count {
  display: flex;
  justify-content: space-between;
  padding: 8rpx 20rpx;
  font-family: SourceHanSansCN-Normal;
	font-size: 28rpx;
  color: #636363;
  line-height: 44rpx;
}
.partnerTeam .table {
  width: 100%;
  font-family: SourceHanSansCN-Normal;
	font-size: 28rpx;
  border-top: 1rpx solid #e9e9e9;
}
.partnerTeam .table .th, .partnerTeam .table .tr {
  display: flex;
  border-bottom: 1rpx solid #e9e9e9;
}
.partnerTeam .table .td {
  border-right: 1rpx solid #e9e9e9;
}
.partnerTeam .table .td:nth-last-child(1) {
  border-right: none;
}
.partnerTeam .table .th .td {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 70rpx;
}
.partnerTeam .table .td1 {
  flex-shrink: 0;
  width: 186rpx;
}
.partnerTeam .table .td2 {
  flex: 1;
  padding: 0 30rpx;
}
.partnerTeam .table .tr .td1 {
  display: flex;
  align-items: center;
  justify-content: center;
  color: #18425a;
}
.partnerTeam .table .tr .td2 {
  padding: 10rpx 30rpx;
}
.partnerTeam .table .tr .td2 .item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  line-height: 60rpx;
  color: #636363;
}
.partnerTeam .table .tr .td2 .item .del {
  color: #18425a;
}
.partnerTeam .bottom_fixed {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: auto;
  display: flex;
  justify-content: space-between;
  font-family: SourceHanSansCN-Normal;
	font-size: 28rpx;
  background: #f8f8f8;
}
.partnerTeam .bottom_fixed view {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 186rpx;
  height: 100rpx;
  color: #18425a;
}
.partnerTeam .bottom_fixed .l {
  border-right: 1rpx solid #e9e9e9;
}
.partnerTeam .bottom_fixed .r {
  border-left: 1rpx solid #e9e9e9;
}