/* pages/trademark/trademarkPlan/trademarkPlan.wxss */
page {
  background: #f8f8f8;
}
.trademarkPlan .company {
  display: flex;
  align-items: center;
  background: #ffffff;
  height: 70rpx;
  padding: 0 30rpx;
  font-family: SourceHanSansCN-Normal;
	font-size: 30rpx;
  border-bottom: 1rpx solid #e9e9e9;
}
.trademarkPlan .company .val {
  color: #9a9a9a;
  margin-left: 15rpx;
}
.trademarkPlan .plan {
  width: 100%;
}
.trademarkPlan .plan .tit {
  display: flex;
  align-items: center;
  height: 90rpx;
  padding: 0 30rpx;
  font-family: SourceHanSansCN-Normal;
	font-size: 30rpx;
}
.trademarkPlan .plan .plan_list {
  padding: 10rpx 30rpx;
  background: #ffffff;
  font-family: SourceHanSansCN-Normal;
	font-size: 30rpx;
  color: #9a9a9a;
  border-bottom: 1rpx solid #e9e9e9;
}
.trademarkPlan .plan .plan_list .item {
  display: flex;
  line-height: 54rpx;
}
.trademarkPlan .plan .plan_list .item .time {
  margin-right: 20rpx;
}
.trademarkPlan .plan .plan_list .item .txt {
  flex: 1;
}
.trademarkPlan .fixed_b {
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 9;
  width: 100%;
  height: 100rpx;
  background: #18425a;
  color: #ffffff;
  display: flex;
  justify-content: space-between;
}
.trademarkPlan .fixed_b view {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 100%;
  width: 200rpx;
  font-family: SourceHanSansCN-Normal;
	font-size: 28rpx;
}

.alert_win {
  bottom: 250rpx;
}
.alert_win .title {
  height: 70rpx;
  font-size: 30rpx;
  display: flex;
  justify-content: space-between;
}
.alert_win .title view {
  display: flex;
  align-items: center;
  width: 214rpx;
  height: 70rpx;
}
.alert_win .title .cancel {
  background: #ffffff;
  font-family: SourceHanSansCN-Normal;
  color: #9a9a9a;
  padding-left: 50rpx;
}
.alert_win .title .confirm {
  justify-content: flex-end;
  padding-right: 50rpx;
}

.alert_win .infor_con {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-family: SourceHanSansCN-Normal;
	font-size: 30rpx;
  color: #9a9a9a;
  width: 100%;
  height: 448rpx;
  margin-top: 15rpx;
}