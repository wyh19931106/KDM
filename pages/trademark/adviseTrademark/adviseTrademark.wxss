/* pages/trademark/adviseTrademark/adviseTrademark.wxss */
.adviseTrademark .query {
  margin: 10rpx;
  height: 68rpx;
  border-radius: 10rpx;
  display: flex;
  align-items: center;
  border: 1rpx solid #e9e9e9;
  overflow: hidden;
  font-size: 30rpx;
}
.adviseTrademark .query .country {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 130rpx;
  height: 100%;
  color: #9a9a9a;
  border-right: 1rpx solid #e9e9e9;
  padding-right: 50rpx;
  background: url('data:image/png;base64,') 93rpx center no-repeat;
  background-size: 22rpx auto;
}
.adviseTrademark .query .country text {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin: 0 0 0 8rpx;
}
.adviseTrademark .query .cate {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 180rpx;
  height: 100%;
  color: #9a9a9a;
  border-right: 1rpx solid #e9e9e9;
  padding-right: 50rpx;
  background: url('data:image/png;base64,') 143rpx center no-repeat;
  background-size: 22rpx auto;
}
.adviseTrademark .query .cate text {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin: 0 0 0 8rpx;
}
.adviseTrademark .query .text {
  flex: 1;
  margin: 0 15rpx;
}
.adviseTrademark .query .btn {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 140rpx;
  height: 100%;
  color: #ffffff;
  background: #18425a;
}
.adviseTrademark .query .btn icon {
  margin-right: 8rpx;
}
.adviseTrademark .advise {
  width: 100%;
}
.adviseTrademark .advise .title {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 60rpx;
  background: #f8f8f8;
  font-family: SourceHanSansCN-Normal;
	font-size: 24rpx;
  color: #9a9a9a;
}
.adviseTrademark .advise .table {
  display: flex;
  flex-wrap: wrap;
  margin: 10rpx;
  border-top: 1rpx solid #e9e9e9;
  border-left: 1rpx solid #e9e9e9;
}
.adviseTrademark .advise .table .td {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 33.33%;
  height: 66rpx;
  font-family: SourceHanSansCN-Normal;
	font-size: 24rpx;
  color: #9a9a9a;
  border-right: 1rpx solid #e9e9e9;
  border-bottom: 1rpx solid #e9e9e9;
}
.adviseTrademark .advise .table .active {
  background: #e9e9e9;
}
.adviseTrademark .fixed_b {
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 9;
  width: 100%;
  height: 100rpx;
  background: #18425a;
  color: #ffffff;
  display: flex;
  justify-content: space-between;
}
.adviseTrademark .fixed_b view {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 100%;
  width: 200rpx;
  font-family: SourceHanSansCN-Normal;
	font-size: 28rpx;
}
.alert_win {
  position: fixed;
  left: 50%;
  bottom: 250rpx;
  transform: translateX(-50%);
  width: 640rpx;
	height: 554rpx; 
  background: #f8f8f8;
  z-index: 99;
  box-shadow: 0px 0px 9rpx 2rpx rgba(0, 0, 0, 0.3);
}
.alert_win .title {
  height: 70rpx;
  font-size: 30rpx;
  display: flex;
  justify-content: space-between;
}
.alert_win .title view {
  display: flex;
  align-items: center;
  width: 214rpx;
  height: 70rpx;
}
.alert_win .title .cancel {
  background: #ffffff;
  font-family: SourceHanSansCN-Normal;
  color: #9a9a9a;
  padding-left: 50rpx;
}
.alert_win .title .confirm {
  justify-content: flex-end;
  padding-right: 50rpx;
}

.alert_win .infor_con {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-family: SourceHanSansCN-Normal;
	font-size: 30rpx;
  color: #9a9a9a;
  width: 100%;
  height: 448rpx;
  margin-top: 15rpx;
}