/* pages/user/favTrademark/favTrademark.wxss */
page {
  background: #f8f8f8;
}
.favTrademark .title {
  display: flex;
  justify-content: center;
  align-items: center;
  font-family: SourceHanSansCN-Medium;
	font-size: 30rpx;
  color: #18425a;
  height: 78rpx;
  background: #ffffff;
  border-bottom: 1rpx solid #e9e9e9;
}
.favTrademark .list {
  background: #f8f8f8;
  padding: 15rpx 15rpx 150rpx;
}
.favTrademark .list .item {
  display: flex;
  height: 220rpx;
  padding: 15rpx;
  border-radius: 10rpx;
  background: #ffffff;
  overflow: hidden;
  margin-bottom: 20rpx;
}
.favTrademark .list .item .img {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 190rpx;
  height: 190rpx;
  overflow: hidden;
}
.favTrademark .list .item .img image {
  width: 100%;
  height: auto;
}
.favTrademark .list .item .inf {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  margin: 0 30rpx;
  overflow: hidden;
}
.favTrademark .list .item .inf view {
  font-family: SourceHanSansCN-Normal;
	font-size: 24rpx;
  color: #9a9a9a;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.favTrademark .list .item .inf .tit {
  font-family: SourceHanSansCN-Normal;
	font-size: 30rpx;
  color: #18425a;
}
.favTrademark .list .item .cancel {
  width: 130rpx;
  font-family: SourceHanSansCN-Normal;
	font-size: 24rpx;
  color: #18425a;
  text-align: right;
  padding-top: 5rpx;
  position: relative;
}
.favTrademark .list .item .cancel .cancel_box {
  position: aboslute;
  top: 0;
  right: 0;
  width: 100%;
  height: auto;
  background: #e9e9e9;
  font-size: 18rpx;
}
.favTrademark .list .item .cancel .cancel_box .tit {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background: #18425a;
  color: #ffffff;
  height: 54rpx;
}
.favTrademark .list .item .cancel .cancel_box .handle {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 54rpx;
  border-top: 1rpx solid #ffffff;
  color: #13394f;
}
.favTrademark .list .item .cancel .cancel_box .handle text {
  display: flex;
  align-items: center;
  height: 100%;
  padding: 0 20rpx;
}

.favTrademark .fixed_b {
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 9;
  width: 100%;
  height: 100rpx;
  background: #18425a;
  color: #ffffff;
  display: flex;
  justify-content: space-between;
}
.favTrademark .fixed_b view {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 100%;
  width: 200rpx;
  font-family: SourceHanSansCN-Normal;
	font-size: 28rpx;
}
