/* pages/patent/patentQuery/patentQuery.wxss */
.patentQuery .query {
  margin: 10rpx;
  height: 68rpx;
  border-radius: 10rpx;
  display: flex;
  align-items: center;
  border: 1rpx solid #e9e9e9;
  overflow: hidden;
  font-size: 30rpx;
}
.patentQuery .query .country {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 130rpx;
  height: 100%;
  color: #9a9a9a;
  border-right: 1rpx solid #e9e9e9;
  padding-right: 50rpx;
  background: url('data:image/png;base64,') 93rpx center no-repeat;
  background-size: 22rpx auto;
}
.patentQuery .query .country text {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin: 0 0 0 8rpx;
}
.patentQuery .query .cate {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 180rpx;
  height: 100%;
  color: #9a9a9a;
  border-right: 1rpx solid #e9e9e9;
  padding-right: 50rpx;
  background: url('data:image/png;base64,') 143rpx center no-repeat;
  background-size: 22rpx auto;
}
.patentQuery .query .cate text {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin: 0 0 0 8rpx;
}
.patentQuery .query .text {
  flex: 1;
  margin: 0 15rpx;
}
.patentQuery .query .btn {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 140rpx;
  height: 100%;
  color: #ffffff;
  background: #18425a;
}
.patentQuery .query .btn icon {
  margin-right: 8rpx;
}
.patentQuery .table {
  border-top: 1rpx solid #e9e9e9;
  margin-top: 10rpx;
  font-family: SourceHanSansCN-Normal;
	font-size: 24rpx;
  color: #9a9a9a;
}
.patentQuery .table .th {
  height: 104rpx;
  display: flex;
}
.patentQuery .table .tr {
  height: 100rpx;
  display: flex;
}
.patentQuery .table .td1 {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 152rpx;
  height: 100%;
  border-bottom: 1rpx solid #e9e9e9;
  border-right: 1rpx solid #e9e9e9;
}
.patentQuery .table .td2 {
  display: flex;
  justify-content: center;
  align-items: center;
  flex: 1;
  border-bottom: 1rpx solid #e9e9e9;
  border-right: 1rpx solid #e9e9e9;
  overflow: hidden;
}
.patentQuery .table .tr .td2 {
  display: flex;
  padding: 0 10rpx;
}
.patentQuery .table .tr .td2 .txt {
  flex: 1;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box; /*（多行）*/
	-webkit-line-clamp: 2;
	-webkit-box-orient: vertical;
  margin-right: 10rpx;
}
.patentQuery .table .tr .td2 .fav_btn {
  width: 80rpx;
   display: flex;
  justify-content: center;
  align-items: center;
  color: #18425a;
}
.patentQuery .table .td3 {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 130rpx;
  height: 100%;
  border-bottom: 1rpx solid #e9e9e9;
}
.patentQuery .table .td3 image {
  width: 70rpx;
  height: auto;
}
.result_win {
  bottom: 200rpx;
}