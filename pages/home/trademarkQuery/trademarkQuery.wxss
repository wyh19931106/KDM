/* pages/trademarkQuery/trademarkQuery.wxss */
.trademarkQuery {
  display: flex;
  flex-direction: column;
  position: fixed;
  min-height: 100%;
  width: 100%;
}
.trademarkQuery .top_show {
  display: block;
  width: 100%;
  height: auto;
}
.trademarkQuery .select_box {
  padding: 104rpx 14rpx;
}
.trademarkQuery .select_box .item {
  border-radius: 10px;
	border: solid 2rpx #e9e9e9;
  height: 97rpx;
  position: relative;
  display: flex;
  align-items:center;
  font-size: 30rpx;
  padding: 0 20rpx;
  overflow: hidden;
}
.trademarkQuery .select_box .item input {
  flex: 1;
}
.trademarkQuery .select_box .select_t {
  display: flex;
  height: 113rpx;
}
.trademarkQuery .select_box .select_t .country {
  width: 225rpx;
	height: 113rpx;
  margin-right: 10rpx;
}
.trademarkQuery .select_box .select_t .country::after {
  content: "";
  position: absolute;
  top: 0;
  right: 0;
  width: 0;
  height: 0;
  border-bottom: 40rpx solid transparent;
  border-right: 22px solid #9a9a9a;
}
.trademarkQuery .select_box .select_t .no {
  flex: 1;
  height: 113rpx;
}
.trademarkQuery .select_box .select_t .no .camera_btn {
  width: 40rpx;
  height: auto;
  margin-left: 15rpx;
}
.trademarkQuery .select_box .select_b {
  margin-top: 10rpx;
  display: flex;
}
.trademarkQuery .select_box .select_b input {
  text-align: center;
}
.trademarkQuery .select_box .select_b::after {
  content: "";
  position: absolute;
  top: 0;
  right: 0;
  width: 0;
  height: 0;
  border-bottom: 40rpx solid transparent;
  border-right: 22px solid #9a9a9a;
}
.trademarkQuery .select {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
}
.trademarkQuery .select_btn {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 268rpx;
	height: 84rpx;
	background-color: #18425a;
	border-radius: 10rpx;
  color: #ffffff;
  font-family: SourceHanSansCN-Medium;
	font-size: 44rpx;
	font-weight: normal;
}
.trademarkQuery .select_btn icon {
  margin-right: 12rpx;
}

.trademarkQuery .fixed_right {
  position: fixed;
  right: 0;
  bottom: 58rpx;
  z-index: 9;
  width: 90rpx;
  height: 150rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.trademarkQuery .fixed_right view {
  padding: 10rpx 20rpx;
}
.trademarkQuery .fixed_right image {
  width: 48rpx;
  height: auto;
}
.trademarkQuery .fixed_right_tel {
  position: fixed;
  right: 10rpx;
  bottom: 58rpx;
  z-index: 9;
  width: 140rpx;
  height: 170rpx;
  display: flex;
  flex-direction: column;
  background: #e9e9e9;
}
.trademarkQuery .fixed_right_tel .item {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 58rpx;
  color: #18425a;
  font-family: SourceHanSansCN-Normal;
	font-size: 20rpx;
  border-bottom: 4rpx solid #ffffff;
}
.trademarkQuery .fixed_right_tel .no {
  color: #ffffff;
  background: #18425a;
}
.trademarkQuery .hint {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-family: SourceHanSansCN-Normal;
	font-size: 24rpx;
	font-weight: normal;
  padding: 50rpx 0;
  color: #9a9a9a;
}
.camera_win {
  background: #ffffff;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.camera_win .item {
  width: 495rpx;
	height: 80rpx;
	background-color: #f8f8f8;
	border-radius: 10px;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-bottom: 15rpx;
  color: #9a9a9a;
  font-family: SourceHanSansCN-Normal;
	font-size: 38rpx;
}
.camera_win .active {
  background: #18425a;
  color: #ffffff;
}
