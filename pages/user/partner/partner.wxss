/* pages/user/partner/partner.wxss */
.partner {
  width: 100%;
}
.partner .top_show {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  width: 100%;
  height: 306rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 100% 100%;
  padding: 20rpx 40rpx;
  color: #ffffff;
}
.partner .top_show .top_inf {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  height: 94rpx;
}
.partner .top_show .top_inf .t_l {
  display: flex;
  align-items: center;
}
.partner .top_show .top_inf .t_l .avatar {
  width: 94rpx;
  height: 94rpx;
  margin-right: 20rpx;
}
.partner .top_show .top_inf .t_l .avatar image {
  width: 100%;
  height: auto;
}
.partner .top_show .top_inf .t_l .name {
  font-family: SourceHanSansCN-Normal;
	font-size: 42rpx;
}
.partner .top_show .top_inf .t_r {
  font-family: SourceHanSansCN-Normal;
	font-size: 28rpx;
}
.partner .top_show .handle {
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-family: SourceHanSansCN-Normal;
	font-size: 28rpx;
}
.partner .top_show .handle .handle_item {
  text-align: center;
}
.partner .top_show .handle .handle_item .no {
  margin-top: 5rpx;
}
.partner .count {
  display: flex;
  width: 100%;
  border-bottom: 1rpx solid #e9e9e9;
}
.partner .count .count_item {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  height: 140rpx;
  font-family: SourceHanSansCN-Normal;
	font-size: 28rpx;
  color: #18425a;
  border-right: 1rpx solid #e9e9e9;
}
.partner .count .count_item:nth-last-child(1) {
  border-right: none;
}
.partner .news {
  display: flex;
  align-items: center;
  height: 140rpx;
  padding: 0 25rpx;
  border-bottom: 1rpx solid #e9e9e9;
}
.partner .news .txt {
  width: 150rpx;
  flex-shrink: 0;
  font-family: SourceHanSansCN-Medium;
	font-size: 36rpx;
  color: #fe4f00;
}
.partner .news .news_swiper {
  flex: 1;
  height: 80rpx;
  overflow: hidden;
}
.partner .news .news_swiper .slide_item {
  display: flex;
  align-items: center;
}
.partner .news .news_swiper .slide_item .val {
  flex: 1;
  height: 100%;
  line-height: 40rpx;
  font-family: SourceHanSansCN-Normal;
	font-size: 28rpx;
  color: #636363;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box; /*（多行）*/
	-webkit-line-clamp: 2;
	-webkit-box-orient: vertical;
  word-break: break-all;
  margin-right: 30rpx;
}
.partner .news .news_swiper .slide_item image {
  width: 15rpx;
  height: auto;
}
.partner .handle_list {
  width: 100%;
  padding: 5rpx 0;
}
.partner .handle_list .handle_item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 64rpx;
  padding: 0 25rpx;
}
.partner .handle_list .handle_item .txt {
  font-family: SourceHanSansCN-Normal;
	font-size: 28rpx;
  color: #636363;
}
.partner .handle_list .handle_item image {
  width: 15rpx;
  height: auto;
}
.partner .share {
  width: 100%;
}
.partner .share image {
  width: 100%;
  height: auto;
}