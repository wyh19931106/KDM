/* pages/trademark/trademarkDetails/trademarkDetails.wxss */
.trademarkDetails .top_show {
  width: 100%;
  height: auto;
}
.trademarkDetails .top_show image {
  width: 100%;
  height: auto;
}
.trademarkDetails .infor {
  padding: 0 20rpx 100rpx 20rpx;
}
.trademarkDetails .infor .item {
  display: flex;
  align-items: center;
  height: 90rpx;
  font-family: SourceHanSansCN-Normal;
	font-size: 26rpx;
  padding: 0 10rpx;
  border-bottom: 1rpx solid #e9e9e9;
}
.trademarkDetails .infor .item .txt {
  color: #000000;
  margin-right: 15rpx;
}
.trademarkDetails .infor .item .val {
  flex: 1;
  overflow: hidden;
  color: #9a9a9a;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.trademarkDetails .infor .item image {
  width: 15rpx;
  height: auto;
  margin-left: 10rpx;
}
.trademarkDetails .infor .justify {
  justify-content: space-between;
}
.trademarkDetails .infor .item .l {
  display: flex;
}
.trademarkDetails .infor .item .r {
  display: flex;
  margin-left: 20rpx;
  align-items: center;
}
.trademarkDetails .fixed_b {
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 9;
  width: 100%;
  height: 100rpx;
  background: #18425a;
  color: #ffffff;
  display: flex;
  justify-content: space-between;
}
.trademarkDetails .fixed_b view {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 100%;
  width: 200rpx;
  font-family: SourceHanSansCN-Normal;
	font-size: 28rpx;
}

.alert_win {
  bottom: 250rpx;
}
.alert_win .title {
  height: 70rpx;
  font-size: 30rpx;
  display: flex;
  justify-content: space-between;
}
.alert_win .title view {
  display: flex;
  align-items: center;
  width: 214rpx;
  height: 70rpx;
}
.alert_win .title .cancel {
  background: #ffffff;
  font-family: SourceHanSansCN-Normal;
  color: #9a9a9a;
  padding-left: 50rpx;
}
.alert_win .title .confirm {
  justify-content: flex-end;
  padding-right: 50rpx;
}

.alert_win .infor_con {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-family: SourceHanSansCN-Normal;
	font-size: 30rpx;
  color: #9a9a9a;
  width: 100%;
  height: 448rpx;
  margin-top: 15rpx;
}